module boolean (
    input alufn[6],               //defining inputs and outputs of module
    input a[16],
    input b[16],
    output bool[16]

  ) {

  always {
  
    case(alufn[3-:4]) {
      1:bool = a&b; // and
      7:bool = a|b; // or
      6:bool = a^b; //xor
      5:bool = a;
      default:bool = 0;
    }
  }
}