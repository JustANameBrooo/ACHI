module boolean (
    input alufn[4],               //defining inputs and outputs of module
    input a[16],
    input b[16],
    output out[16]

  ) {

  always {
  
    case (alufn) {
      // TODO: use a mux instead
      4b1000: out = a & b;  // and
      4b1110: out = a | b;  // or
      4b0110: out = a ^ b;  // xor
      4b1010: out = a;  // a
      4b1100: out = b;  // b
      default: out = 0;
    }
  }
}