module alu (  // 16 bit ALU unit
    // input clk,  // clock
    // input rst,  // reset
    input alufn[6],
    input a[16],
    input b[16],
    output out[16]
  ) {
  
  // alu modules go here
  boolean boole;

  always {
  //bool
    boole.a = a;
    boole.b = b;
    boole.alufn = alufn;
    //to be made with multiplexer (if bool is selected)
    out = boole.bool;
  }
}

